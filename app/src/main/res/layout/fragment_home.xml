<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_3"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/welcome"
        android:layout_width="387dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/coustard"
        android:text="What are you grateful for today?"
        android:textColor="#060010"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/health2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/health"
        android:padding="20dp"
        app:layout_constraintEnd_toStartOf="@+id/money2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/career2" />

    <Button
        android:id="@+id/money2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/money"
        android:padding="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/health2"
        app:layout_constraintTop_toBottomOf="@+id/family2" />

    <Button
        android:id="@+id/family2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/family"
        android:padding="20dp"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/career2"
        app:layout_constraintTop_toBottomOf="@+id/welcome" />

    <Button
        android:id="@+id/knowledge2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/knowledge"
        android:padding="20dp"
        app:layout_constraintEnd_toStartOf="@+id/miscellaneous2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/health2" />

    <Button
        android:id="@+id/miscellaneous2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/misc"
        android:padding="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/knowledge2"
        app:layout_constraintTop_toBottomOf="@+id/money2" />

    <Button
        android:id="@+id/career2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/career"
        android:padding="20dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/family2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome" />

    <Button
        android:id="@+id/review"
        android:layout_width="112dp"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:background="#6B33CD"
        android:fontFamily="casual"
        android:text="Review"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/knowledge2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="140dp"
        android:layout_height="25dp"
        android:layout_marginBottom="1dp"
        android:fontFamily="casual"
        android:text="My Career/Job"
        android:textColor="#10012B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/career2"
        app:layout_constraintEnd_toEndOf="@+id/career2"
        app:layout_constraintStart_toStartOf="@+id/career2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="190dp"
        android:layout_height="25dp"
        android:fontFamily="casual"
        android:text="Family/Relationships"
        android:textColor="#10012B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/family2"
        app:layout_constraintEnd_toEndOf="@+id/family2"
        app:layout_constraintStart_toStartOf="@+id/family2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="130dp"
        android:layout_height="25dp"
        android:fontFamily="casual"
        android:text="My Health"
        android:textColor="#10012B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/health2"
        app:layout_constraintEnd_toEndOf="@+id/health2"
        app:layout_constraintStart_toStartOf="@+id/health2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="130dp"
        android:layout_height="25dp"
        android:fontFamily="casual"
        android:text="My Money/Finances"
        android:textColor="#10012B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/money2"
        app:layout_constraintEnd_toEndOf="@+id/money2"
        app:layout_constraintStart_toStartOf="@+id/money2" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="130dp"
        android:layout_height="25dp"
        android:fontFamily="casual"
        android:text="Knowledge"
        android:textColor="#10012B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/knowledge2"
        app:layout_constraintEnd_toEndOf="@+id/knowledge2"
        app:layout_constraintStart_toStartOf="@+id/knowledge2" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="130dp"
        android:layout_height="25dp"
        android:fontFamily="casual"
        android:text="Miscellaneous"
        android:textColor="#10012B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/miscellaneous2"
        app:layout_constraintEnd_toEndOf="@+id/miscellaneous2"
        app:layout_constraintStart_toStartOf="@+id/miscellaneous2" />

</androidx.constraintlayout.widget.ConstraintLayout>